	\"creditRequirements\": {\"${sia}\": 140, \"${derecho}\": 144, \"${ss}\": 140, \"${pf}\": 160, \"${soa}\": 140, \"${forma3}\": 168 } \
	}" localhost:3000/api/university/programs/${programId1}/courses-collection


# Define course requirements
